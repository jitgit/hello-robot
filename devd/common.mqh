#property strict
input bool DEBUG_INFO = false;
input bool WARN_INFO = true;

//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
void log(string s)
{
    Print("INFO - " + s);
}

//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
void debug(string s)
{
    if (DEBUG_INFO)
    {
        Print("DBUG - " + s);
    }
}

void warn(string s)
{
    if (WARN_INFO)
    {
        Print("WARN - " + s);
    }
}
//+------------------------------------------------------------------+
